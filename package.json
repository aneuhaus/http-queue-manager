{
  "name": "http-queue-manager",
  "version": "0.1.0",
  "description": "A TypeScript HTTP queue manager with retry and backpressure features",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "test:integration": "bun test --preload ./tests/setup.ts tests/integration",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "ioredis": "^5.4.1",
    "pg": "^8.13.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/pg": "^8.11.14",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "queue",
    "http",
    "retry",
    "backpressure",
    "redis",
    "postgresql",
    "bun"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aneuhaus/http-queue-manager.git"
  }
}
